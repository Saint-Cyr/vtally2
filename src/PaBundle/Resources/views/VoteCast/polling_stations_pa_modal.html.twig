
                <div class="row">
                    <div class="col-lg-12">
                        <div class="panel panel-light">
                            <div class="panel-heading" style="font-weight: bold; font-size: 23px;">Parliamentary:<span style="color: blue;"> {{ pollingStation.name }} </span></div>
                            <div class="panel-body">
                                <div class="table-responsive">
                                    <table class="table table-striped table-bordered table-hover">
                                        <thead>
                                        <tr>
                                            <th>Rang</th>
                                            <th>Winner</th>
                                            <th>Party</th>
                                            <th>Vote cast</th>
                                        </tr>
                                        </thead>
                                        <tbody>

                                       

                                    {% for object in candidates %}
                                        <tr>
                                            {% if object.order == 1 %}
                                                <td style="color: green; font-weight: bold; font-size: 23px;">
                                                    {{ object.order }}<sup>st</sup>
                                                </td>
                                                <td style="color: green; font-weight: bold; font-size: 17px;">
                                                    {% if object.isIndependentCandidate %} <img src="{{ asset('upload/images/parIndCandidate/'~object.image) }}" width="50px"/> {% else %} 
                                                        <img src="{{ asset('upload/images/parDepCandidate/'~object.image) }}" width="50px"/>
                                                    {% endif %}
                                                     {{ object.firstName }} {{ object.lastName }} 
                                                </td>
                                                <td style="color: green; font-weight: bold; font-size: 17px;">
                                                    {% if object.isIndependentCandidate %} IC {% else %} 
                                                        <img src="{{ asset('upload/images/paParty/'~object.paParty.image) }}" width="50px"/> 
                                                        {{ object.paParty }}
                                                    {% endif %}
                                                </td>
                                                <td style="color: green; font-weight: bold; font-size: 23px;">
                                                    {{ object.voteCast }}
                                                </td>
                                            {% elseif object.order == 2 %}
                                                <td style="color: orange; font-weight: bold; font-size: 23px;">
                                                    {{ object.order }}<sup>nd</sup>
                                                </td>
                                                <td style="color: orange; font-weight: bold; font-size: 17px;">
                                                    {% if object.isIndependentCandidate %} <img src="{{ asset('upload/images/parIndCandidate/'~object.image) }}" width="50px"/> {% else %} 
                                                        <img src="{{ asset('upload/images/parDepCandidate/'~object.image) }}" width="50px"/>
                                                    {% endif %}
                                                     {{ object.firstName }} {{ object.lastName }} 
                                                </td>
                                                <td style="color: orange; font-weight: bold; font-size: 17px;">
                                                    {% if object.isIndependentCandidate %} IC {% else %} 
                                                        <img src="{{ asset('upload/images/paParty/'~object.paParty.image) }}" width="50px"/> 
                                                        {{ object.paParty }}
                                                    {% endif %}
                                                </td>
                                                <td style="color: green; font-weight: bold; font-size: 23px;">
                                                    {{ object.voteCast }}
                                                </td>
                                            {% elseif object.order == 3 %}
                                                <td style="color: red; font-weight: bold; font-size: 23px;">
                                                    {{ object.order }}<sup>rd</sup>
                                                </td>
                                                <td style="color: red; font-weight: bold; font-size: 17px;">
                                                    {% if object.isIndependentCandidate %} <img src="{{ asset('upload/images/parIndCandidate/'~object.image) }}" width="50px"/> {% else %} 
                                                        <img src="{{ asset('upload/images/parDepCandidate/'~object.image) }}" width="50px"/>
                                                    {% endif %}
                                                     {{ object.firstName }} {{ object.lastName }} 
                                                </td>
                                                <td style="color: red; font-weight: bold; font-size: 17px;">
                                                    {% if object.isIndependentCandidate %} IC {% else %} 
                                                        <img src="{{ asset('upload/images/paParty/'~object.paParty.image) }}" width="50px"/> 
                                                        {{ object.paParty }}
                                                    {% endif %}
                                                </td>
                                                <td style="color: red; font-weight: bold; font-size: 23px;">
                                                    {{ object.voteCast }}
                                                </td>
                                            {% else %}
                                                <td style="color: black; font-weight: bold; font-size: 23px;">
                                                    {{ object.order }}<sup>th</sup>
                                                </td>
                                                <td style="color: black; font-weight: bold; font-size: 17px;">
                                                    {% if object.isIndependentCandidate %} <img src="{{ asset('upload/images/parIndCandidate/'~object.image) }}" width="50px"/> {% else %} 
                                                        <img src="{{ asset('upload/images/parDepCandidate/'~object.image) }}" width="50px"/>
                                                    {% endif %}
                                                     {{ object.firstName }} {{ object.lastName }} 
                                                </td>
                                                <td style="color: black; font-weight: bold; font-size: 17px;">
                                                    {% if object.isIndependentCandidate %} IC {% else %} 
                                                        <img src="{{ asset('upload/images/paParty/'~object.paParty.image) }}" width="50px"/> 
                                                        {{ object.paParty }}
                                                    {% endif %}
                                                </td>
                                                <td style="color: black; font-weight: bold; font-size: 23px;">
                                                    {{ object.voteCast }}
                                                </td>
                                            {% endif %}
                                        </tr>
                                        {% endfor %}
                                    

                                        </tbody>
                                    </table>
                                    <div class="modal-footer">
                                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    
                 
                                            
                                            
                                            
                                 
