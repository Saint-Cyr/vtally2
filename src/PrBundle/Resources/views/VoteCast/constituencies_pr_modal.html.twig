
                <div class="row">
                    <div class="col-lg-12">
                        <div class="panel panel-light">
                            <div class="panel-heading" style="font-weight: bold; font-size: 23px;">Presidential - Constituency:<span style="color: blue;"> {{ constituency.name }}</span></div>
                            <div class="panel-body">
                                <div class="table-responsive">
                                    <table class="table table-striped table-bordered table-hover">
                                        <thead>
                                        <tr>
                                            <th>Rang</th>
                                            <th>Party</th>
                                            <th>Vote Cast</th>
                                            <th>Percentage % </th>
                                        </tr>
                                        </thead>
                                        <tbody>
                                        {% for object in prVoteCasts %}
                                                <tr>
                                                    {% if object.order == 1 %}
                                                        <td style="color: green; font-weight: bold; font-size: 23px;">{{ object.order }}<sup>st</sup></td>
                                                        <td style="color: green; font-weight: bold; font-size: 23px;">
                                                        <img src="{{ asset('upload/images/prParty/'~object.image) }}" width="50px"/> {{ object.name }}</td>
                                                        <td style="color: green; font-weight: bold; font-size: 23px;">{{ object.voteCast }}</td>
                                                        <td style="color: green; font-weight: bold; font-size: 23px;">--</td>
                                                    {% elseif object.order == 2 %}
                                                        <td style="color: orange; font-weight: bold; font-size: 23px;">{{ object.order }}<sup>nd</sup></td>
                                                        <td style="color: orange; font-weight: bold; font-size: 23px;">
                                                        <img src="{{ asset('upload/images/prParty/'~object.image) }}" width="50px"/> {{ object.name }}</td>
                                                        <td style="color: orange; font-weight: bold; font-size: 23px;">{{ object.voteCast }}</td>
                                                        <td style="color: orange; font-weight: bold; font-size: 23px;">--</td>
                                                    {% elseif object.order == 3 %}
                                                        <td style="color: red; font-weight: bold; font-size: 23px;">{{ object.order }}<sup>rd</sup></td>
                                                        <td style="color: red; font-weight: bold; font-size: 23px;">
                                                        <img src="{{ asset('upload/images/prParty/'~object.image) }}" width="50px"/> {{ object.name }}</td>
                                                        <td style="color: red; font-weight: bold; font-size: 23px;">{{ object.voteCast }}</td>
                                                        <td style="color: red; font-weight: bold; font-size: 23px;">--</td>
                                                    {% else %}
                                                        <td style="color: black; font-weight: bold; font-size: 23px;">{{ object.order }}<sup>th</sup></td>
                                                        <td style="color: black; font-weight: bold; font-size: 23px;">
                                                        <img src="{{ asset('upload/images/prParty/'~object.image) }}" width="50px"/> {{ object.name }}</td>
                                                        <td style="color: black; font-weight: bold; font-size: 23px;">{{ object.voteCast }}</td>
                                                        <td style="color: black; font-weight: bold; font-size: 23px;">--</td>
                                                    {% endif %}
                                                        
                                                </tr>
                                            {% endfor %}
                                        </tbody>
                                    </table>
                                    <div class="modal-footer">
                                        
                                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    
                 
                                            
                                            
                                            
                                 
